{
  "manifest_version": 3,
  "name": "Trademark Scraper",
  "version": "1.0",
  "description": "Scrape trademark data using Puppeteer",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
